#!/usr/bin/env node

// console.info('Node Version', process.version);

if (!process.argv[2]) {
  console.error('Usage: ' + process.argv[1] + ' [filename]')
  return;
}

const fs = require('fs');
const fileName = process.argv[2];

var json = JSON.parse(fs.readFileSync(fileName, 'utf8'));

console.log(json);
